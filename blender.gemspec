Gem::Specification.new do |s|
   s.name = %q{blender}
   s.version = "1.0"
   s.date = %q{2008-05-15}
   s.authors = ["Blake Elshire & Chris Griego"]
   s.email = %q{belshire@gmail.com}
   s.summary = %q{Blender gives you production-ready CSS and/or JavaScript assets.}
   s.homepage = %q{http://github.com/cgriego/front-end-blender/tree/master}
   s.description = %q{Blender is like Make or Ant for the front-end. It aggregates and compresses CSS and/or JavaScript assets of a site into production-ready files.}
   s.files = [ "README", "blender", "bin/yuicompressor.jar"]
end